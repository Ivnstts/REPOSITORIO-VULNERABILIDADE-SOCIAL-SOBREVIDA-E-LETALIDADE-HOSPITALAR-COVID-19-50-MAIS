library(survival)
library(survminer)

res.cox <- coxph(Surv(DIAS_SINTOMA_MORTE, MORTE) ~  GeoSES+CS_SEXO+NU_IDADE_N+COR+CLASSE_MULTIMORBIDADE+ENTUBADO, data = CORTE_100_DIAS)
res.cox
summary(res.cox)
AIC(res.cox)

fit <- survfit(Surv(DIAS_SINTOMA_MORTE, MORTE) ~ GeoSES, data = CORTE_100_DIAS)
ggsurvplot(fit,
           pval = TRUE, conf.int = TRUE,
           linetype = "strata", # Change line type by groups
           surv.median.line = "hv", 
           break.time.by = 2)
